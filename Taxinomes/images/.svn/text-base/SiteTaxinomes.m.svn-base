#pragma mark - View lifecycle


- (void)viewDidLoad
{
    [super viewDidLoad];
    //adresse des taxinomes
    NSString *adresseUrl = @"http://www.lestaxinomes.org/";
    //que l'on met dans l'objet nsurl
    NSURL *url = [NSURL URLWithString:adresseUrl];
    //et voila la requete
    NSURLRequest *requestObj = [NSURLRequest requestWithURL:url];
    //definition de la webview
    [page_taxinomes loadRequest:requestObj];
    // Do any additional setup after loading the view from its nib.
}

